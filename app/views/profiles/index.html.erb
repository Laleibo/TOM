<div class="header">
  <div class="logo">
    LOGO
  </div>
  <div class="sign_in">
    <%= form_tag sessions_path do %>
     <span class="username_in">
      <%= label_tag :email %>
      <%= text_field_tag :email %>
    </span>
    <span class="password_in">
      <%= label_tag :password %>
      <%= password_field_tag :password_digest %>
    </span>

 <p id="notice"><%= notice %></p>

<h1>Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Password digest</th>
      <th>Address1</th>
      <th>Address2</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.email %></td>
        <td><%= profile.password_digest %></td>
        <td><%= profile.address1 %></td>
        <td><%= profile.address2 %></td>
        <td><%= profile.city %></td>
        <td><%= profile.state %></td>
        <td><%= profile.zip %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </div>
  <div class="random">Would you like to sign up? <span class="create"> Click Here
  </span></div>
  <div class="log_in">
    Log In
  </div>

</div>
<div class="index_pic">This is where the picture goes</div>
<div class="create_form">
  <button id="create_form_exit">X</button>
  <%= render 'new', :controller => 'sessions' :action => 'new'%>
</div>
<div class="steps">

</div>
